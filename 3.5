public class MatrixProduct 
{
    int[][] matrix1;
    int[][] matrix2;

    public MatrixProduct() 
    {
        matrix1 = new int[][]{{1, 2}, {3, 4}};
        matrix2 = new int[][]{{5, 6}, {7, 8}};
    }

    public int[][] multiplyMatrices()
    {
        int[][] product = new int[2][2];
        for (int i = 0; i < 2; i++) 
        {
            for (int j = 0; j < 2; j++) 
            {
                product[i][j] = 0;
                for (int k = 0; k < 2; k++) 
                {
                    product[i][j] += matrix1[i][k] * matrix2[k][j];
                }
            }
        }
        return product;
    }

    public static void main(String[] args)
    {
        MatrixProduct mp = new MatrixProduct();
        int[][] result = mp.multiplyMatrices();
        for (int i = 0; i < 2; i++) 
        {
            for (int j = 0; j < 2; j++) 
            {
                System.out.print(result[i][j] + " ");
            }
            System.out.println();
        }
    }
}
